{
    "name": "@farisaziz12/web-core",
    "version": "1.1.0",
    "description": "Reusable packages to make website building faster",
    "scripts": {
        "test": "jest --passWithNoTests",
        "release": "standard-version",
        "deploy": "npm publish",
        "lint": "prettier --check .",
        "format": "prettier --write .",
        "compile": "tsc"
    },
    "husky": {
        "hooks": {
            "pre-commit": "prettier --write . && git add -A .",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/farisaziz12/web-core.git"
    },
    "author": "Faris Aziz",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/farisaziz12/web-core/issues"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com"
    },
    "homepage": "https://github.com/farisaziz12/web-core#readme",
    "devDependencies": {
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-conventional": "^11.0.0",
        "husky": "^5.0.0-beta.0",
        "jest": "^26.6.3",
        "prettier": "2.1.2",
        "standard-version": "^9.0.0"
    },
    "dependencies": {
        "firebase": "^8.0.2"
    }
}
